{
  "name": "dbox-repo",
  "displayName": "DBox Repository",
  "description": "Your personal VS Code Extensions Repository",
  "version": "0.0.2",
  "publisher": "dteam-top",
  "icon": "pages/shared/imgs/logo.png",
  "engines": {
    "vscode": "^1.42.0"
  },
  "keywords": [
    "vscode",
    "dbox",
    "extension",
    "repository",
    "marketplace"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dbox.showExtensions",
        "title": "%cmd.showExtensions.title%"
      },
      {
        "command": "dbox.showEnabled",
        "title": "%cmd.showEnabled.title%"
      },
      {
        "command": "dbox.showDisabled",
        "title": "%cmd.showDisabled.title%"
      },
      {
        "command": "dbox.home",
        "title": "%cmd.home.title%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "premocha": "npm run compile && npm run lint",
    "mocha": "mocha out/test/*.test.js",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run check"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.4",
    "@types/denodeify": "^1.2.31",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^10.0.3",
    "@types/node-fetch": "^2.5.5",
    "@types/osenv": "^0.1.0",
    "@types/tmp": "^0.1.0",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "~3.7.0",
    "vscode-test": "^1.3.0",
    "gts": "^1.1.2"
  },
  "dependencies": {
    "ali-oss": "6.5.1",
    "aws-sdk": "^2.814.0",
    "denodeify": "1.2.1",
    "dropbox": "4.0.30",
    "fs-extra": "^8.1.0",
    "handlebars": "4.7.3",
    "node-fetch": "2.6.0",
    "osenv": "0.1.5",
    "tmp": "0.1.0",
    "vscode-page": "0.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTeam-Top/dbox-repo.git"
  },
  "homepage": "https://github.com/DTeam-Top/dbox-repo/blob/master/README.md",
  "bugs": "https://github.com/DTeam-Top/dbox-repo/issues"
}
